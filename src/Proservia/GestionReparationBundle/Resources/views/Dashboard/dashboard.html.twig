{% extends '::base.html.twig' %}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/morris/morris.css') }}">
        <link rel="stylesheet" href="{{ asset('css/fullcalendar/fullcalendar.css') }}">
        <link rel="stylesheet" href="{{ asset('css/jvectormap/jquery-jvectormap-1.2.2.css') }}">
    {% endblock %}
    {% block title %} Accueil - Dashboard {% endblock %}
    {% block title2 %} Accueil {% endblock %}
    {% block title3 %} Dashboard {% endblock %}
    {% block breadcrumb %} Dashboard {% endblock %}
{% block body %}

    <div class="col-sm-12" id="dashboard-graphs">
        {% embed '@ProserviaGestionReparation/Dashboard/header.html.twig'  %}
        {% endembed %}
    </div>
    <section class="col-sm-12 connectedSortable">
        <div class="nav-tabs-custom">
            <div class="tab-content no-padding">
                {#<div class="col-sm-8" id="dashboard-calendar">#}
                {#{% embed 'DashboardBundle:Default:calendar.html.twig'  %}#}
                {#{% endembed %}#}
                {#</div>#}
                {#<div class="col-sm-4" id="dashboard-right">#}
                {#{% embed 'DashboardBundle:Default:latest_members.html.twig'  %}#}
                {#{% endembed %}#}
                {#{% embed 'DashboardBundle:Default:todo_list.html.twig'  %}#}
                {#{% endembed %}#}
                {#</div>#}
                <div class="col-sm-12" id="dashboard-graphs_ticketsByWeeks">
                    {% embed '@ProserviaGestionReparation/Dashboard/graph_ticketsByWeeks.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-12" id="dashboard-graphs_stockTicketsByWeeks">
                    {% embed '@ProserviaGestionReparation/Dashboard/graph_stockTicketsByWeeks.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-4" id="dashboard-graphs_end_warranty">
                    {% embed '@ProserviaGestionReparation/Dashboard/graph_ticketsByEndWarranty.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-4" id="dashboard-graphs_ticketsBStatus">
                    {% embed 'ProserviaGestionReparationBundle:Dashboard:graph_ticketsByStatus.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-4" id="dashboard-graphs_ticketsByTypePanne">
                    {% embed 'ProserviaGestionReparationBundle:Dashboard:graph_ticketsByTypePanne.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-5" id="dashboard-graphs_ticketsByModel">
                    {% embed 'ProserviaGestionReparationBundle:Dashboard:graph_ticketsByModel.html.twig'  %}
                    {% endembed %}
                </div>
                <div class="col-sm-7" id="dashboard-graphs_ticketsByModelCategory">
                    {% embed 'ProserviaGestionReparationBundle:Dashboard:graph_ticketsByModelCategory.html.twig'  %}
                    {% endembed %}
                </div>
            </div>
        </div>
    </section>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/raphael/raphael-min.js') }}"></script>
    <script src="{{ asset('js/morris/morris.min.js') }}"></script>
    <script src="{{ asset('js/echarts/echarts-all.js') }}"></script>
    <script src="{{ asset('js/sparkline/jquery.sparkline.min.js') }}"></script>
    <script src="{{ asset('js/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
    <script src="{{ asset('js/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    <script src="{{ asset('js/moment/moment.min.js') }}"></script>
    <script src="{{ asset('js/slimScroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('js/fullcalendar/fullcalendar.js') }}"></script>
    <script src="{{ asset('js/fullcalendar/lang-all.js') }}"></script>
    <script src="{{ asset('js/fastclick/fastclick.js') }}"></script>
    <script src="{{ asset('js/dashboard/dashboard.js') }}"></script>
{% endblock %}
